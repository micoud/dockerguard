{
  "routes_allowed": [
    {
      "method": "GET",
      "pattern": "^/containers/json$",
      "append_filter": [
        {
          "filter_key": "network",
          "values": ["jenkins"]
        }
      ]
    },
    {
      "method": "GET",
      "pattern": "^/services$",
      "append_filter": [
        {
          "filter_key": "name",
          "values": ["acs"]
        }
      ]
    },
    {
      "method": "POST",
      "pattern": "^/services/create$",
      "check_json": [
        {
          "key": ["Name"],
          "allowed_values": ["acs"]
        },
        {
          "key": ["TaskTemplate", "Networks"],
          "allowed_values": [
            {"Target": "jenkins"}
          ]
        }
      ]
    },
    {
      "method": "DELETE",
      "pattern": "^/services/(\\w{1,64})$"
    },
    {
      "method": "POST",
      "pattern": "^/services/(\\w{1,64})/update$"
    },
    {
      "method": "GET",
      "pattern": "^/services/(acs.*)$"
    },
    {
      "method": "GET",
      "pattern": "^/tasks$"
    },
    {
      "method": "GET",
      "pattern": "^/nodes$"
    },
    {
      "method": "GET",
      "pattern": "^/networks/jenkins$"
    },
    {
      "method": "GET",
      "pattern": "^/networks$",
      "append_filter": [
        {
          "filter_key": "name",
          "values": ["jenkins"]
        }
      ]
    },
    {
      "method": "GET",
      "pattern": "^/distribution/registry.cta-test.zeuthen.desy.de/(.*actl.*)/json$"
    },
    {
      "method": "GET",
      "pattern": "^/secrets$"
    },
    {
      "method": "GET",
      "pattern": "^/configs$"
    }
  ]
}
