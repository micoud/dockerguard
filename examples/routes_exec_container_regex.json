{
  "routes_allowed": [
    {
      "method": "GET",
      "pattern": "^/containers/json$"
    },
    {
      "method": "*",
      "pattern": "^/containers/(.*mariadb.*)/(json|start|stop|exec)$"
    },
    {
      "method": "*",
      "pattern": "^/exec/(\\w{1,64})/(start|json|resize)$"
    }
  ]
}
